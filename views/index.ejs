<%- include('partials/header') %>

<h1>Displaying all the Blogs</h1>
<a href="/blog/new">NEW</a>
<article>
    <% for(let blog of allBlogs){ %>
        <div>
            <h1><%=blog.id%></h1>
            <h1><%=blog.title%></h1>
            <h1><%=blog.author%></h1>
            <p><%=blog.comment%></p>
            <p> <a href="/blogs/<%=blog.id%>">Show</a> </p>
        </div>
   <% } %>
</article>

<%- include('partials/footer') %>
